  transform: translateY(1px);
  line-height: 22px;
}
.car-slider-box .car-rent-button span small {
  font-size: 15px;
  line-height: 30px;
  opacity: 0.6;
  line-height: 18px;
  transform: translateY(1px);
}
.arrow-main {
  display: flex;
  justify-content: space-between;
  width: 98%;
  margin: auto;
  left: 0;
  right: 0;
  pointer-events: all;
}
.arrow-main button {
  pointer-events: all;
  width: 54px;
  height: 54px;
  border: 1px solid #fff;
  border-radius: 100%;
  cursor: pointer;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0px 4px 4px rgb(0 0 0 / 25%);
  z-index: 99;
}
.drive-now .arrow-main > button {
  top: 35%;
  position: absolute;
}
.arrow-main button .fa {
  font-size: 34px;
}
.arrow-left {
  left: 20px;
}
.arrow-right {
  right: 20px;
}
.show-left-arrow,
.show-right-arrow {
  opacity: 1;
  display: block !important;
  /* pointer-events: all !important; */
