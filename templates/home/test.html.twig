}
.drive-now .car-slider-box {
  background: #212121;
  border-radius: 16px;
  overflow: hidden;
}
.car-slider-box .car-title-img {
  position: relative;
  background-color: #fff;
  height: 424px;
}
.car-slider-box .car-title-img:before {
  position: absolute;
  top: 0;
  left: -3px;
  right: 0;
  /* background-image: url(../images/image-header-shadow.svg); */
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
  z-index: 1;
  content: "";
  display: inline-block;
  width: 102%;
  height: 100px;
  pointer-events: none;
}
.car-slider-box .car-title-img a {
  display: block;
  height: 100% !important;
}
.drive-now .car-slider-box .car-title-img img {
  object-fit: cover;
  object-position: bottom;
  width: 100%;
  height: 100%;
  background: #212121;
  border-top-left-radius: 16px;
  border-top-right-radius: 16px;
}
.drive-now .car-slider-box .car-rent-button span.Car-comp {
